unit clsUSER_u;

interface

uses  SysUtils, clsADDRESS_u;

type
  TUser = class(TObject)
  private
    first_name: String;
    surname: String;
    username: String;
    password: String;
    email: String;
    address: String;

  public
  constructor Create(first_name, surname, username, password, email: string; address_id: integer);
  procedure InsertUserRecord;
  end;

implementation

end.
