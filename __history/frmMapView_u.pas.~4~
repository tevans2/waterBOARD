type
  TfrmMapView = class(TForm)
    pnlHomePage: TPanel;
    Panel1: TPanel;
    Panel2: TPanel;
    Shape1: TShape;
    Panel3: TPanel;
    Image1: TImage;
    Shape2: TShape;
    Image2: TImage;
    procedure FormClose(Sender: TObject; var Action: TCloseAction);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  frmMapView: TfrmMapView;

uses frmHomePage_u;

implementation

{$R *.dfm}

procedure TfrmMapView.FormClose(Sender: TObject; var Action: TCloseAction);
begin
